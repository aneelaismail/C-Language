#include<stdio.h>
int main()
{
	char x,y,z;
	printf("Enter the Alphabate :");
	scanf("%c",&x);
if(x>='A'&& x<='Z')
{
	y=x+32;
 	printf("The Alphabate is changed in lowercase %c",y);
}
 else if (x>='a'&& x<='z')

{
 z=x-32;


	printf("The Alphabate is changed in Uppercase letters %c",z);
}
else {
	printf("Invalid Value ");
}
}
